<article id="{{parent}}-{{name}}">
    <header>
        <h3>{{name}}</h3>
        <code>{{typeName}}</code>
    </header>
    {{{ descriptionHTML }}}
    {{#if params}}
        <article class="parameters">
            <h4>Parameters</h4>
            <ul>
                {{#each params}}
                    <li>
                        {{> param}}
                    </li>
                {{/each}}
            </ul>
        </article>
    {{/if}}
    {{#if returns}}
        <h4>Returns</h4>
    {{/if}}
    {{#each returns}}
        <article class="return">
            <code class="type">{{typeName}}</code>:
            {{{ descriptionHTML }}}
        </article>
    {{/each}}
    {{#if examples}}
        <h4>Example</h4>
    {{/if}}
    {{#each examples}}
        <article class="example">
            <pre><code>
                {{~description}}
            </code></pre>
        </article>
    {{/each}}
</article>
